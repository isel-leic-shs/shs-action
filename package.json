{
  "name": "shs-analyser",
  "version": "1.0.0",
  "private": true,
  "description": "GitHub Action that handles security scans using dependency-check and publish output to SHS service",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test",
    "changeset:create": "changeset",
    "changeset:status": "changeset status --verbose --output",
    "changeset:release": "changeset version"
  },
  "keywords": [
    "github-action"
  ],
  "license": "MB-ISL",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@actions/io": "^1.1.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.10.5",
    "@typescript-eslint/parser": "^5.8.1",
    "@vercel/ncc": "^0.31.1",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^25.3.2",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4"
  }
}
